{
  "manifest_version": 3,
  "name": "Tab Flow - Visual Tab Manager & Switcher",
  "short_name": "Tab Flow",
  "version": "1.1.0",
  "description": "A visual Tab Flow with thumbnail previews - navigate tabs with keyboard shortcuts, mouse clicks, or arrow keys",
  "homepage_url": "https://github.com/prabhjot0109/TabFlow",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "activeTab",
    "storage",
    "scripting",
    "sessions",
    "favicon",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader-H9IaCqIx.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "commands": {
    "quick-switch": {
      "suggested_key": {
        "default": "Alt+Q",
        "windows": "Alt+Q",
        "mac": "Alt+Q"
      },
      "description": "Quick tab switch (Alt+Tab style - instant)"
    },
    "show-tab-flow": {
      "suggested_key": {
        "default": "Alt+W",
        "windows": "Alt+W",
        "mac": "Alt+W"
      },
      "description": "Show Tab Flow with search"
    },
    "cycle-next-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "windows": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Cycle to next tab in Flow"
    }
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "_favicon/*",
        "src/flow/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/index.ts-DFSbDPE5.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
